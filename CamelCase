//not the whole code.
public class Solution {

    // Complete the camelcase function below.
    static int camelcase(String s) {
        int c=1;
        for(int i=0;i<s.length();i++){
            String letter = Character.toString(s.charAt(i));
                if(letter == letter.toUpperCase())
                c++;
            }
    
    return c;
    }
